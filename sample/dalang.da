#!da

// Just a da-lang sample code.

function bar(flag, times) {
  hint1 = "Hello, World."
  hint2 = 'Hello, DaLang.'

  for _ : times {
    if flag {
     print(hint1)
     print(' from bar\n')
    } else {
      print(hint2)
      print(" from bar\n")
    }
  }
}

class thud() {}

class qux(thud) {
  function baz(flag,
               times) {
    hint1 = "Hello, World."
  hint2 = 'Hello, DaLang.'

    i = 0
    while i < times {
      i += 1
      if flag {
        print(hint1 + ' from baz\n')
      } else {
        print(hint2 + " from baz\n")
      }
    }
  }
}

function foo(flag=true) {
  x = 6
 y = 4
  z = 2

  bar(flag, 2)
  qux().baz(flag, 3)

  if flag { return x + y / z
  } else {
    return (x - y) * 2
  }
}

function main() {
  foo() # Call foo function directly.
}