#!dalang
#perf record -g -e cpu-cycles ./da -v ../sample/fibonacci.da

function fibonacci(n) {
    if (n <= 1) {
        return n
    }
    return fibonacci(n - 1) + fibonacci(n - 2)
}

function main() {
    n = 50 : int
    i = 0 : int
    while i < n {
      i = i + 1
      <: '#' <: i <: ': ' <: fibonacci(i) <: '\n'
    }
}

<: 'fib:\n'
main()
<: '\n'